{
  "name": "student_assistant",
  "version": "0.1.0",
  "author": "Ваше Имя evgenij.semenov.1998@bk.ru",
  "private": true,
  "main": "electron/main.js",
  "productName": "student_assistant",
  "homepage": "./",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "animate.css": "^4.1.1",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "require": "^0.4.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "package-win": "electron-builder --win",
    "package-linux": "electron-builder --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "electron": "^29.1.5",
    "electron-builder": "^24.12.0",
    "sass": "^1.77.6"
  },
  "build": {
    "appId": "com.example.studenthelper",
    "productName": "Помощник студента",
    "copyright": "Copyright © 2023 Ваше Имя",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "electron/ico/icon.ico",
      "legalTrademarks": "Помощник студента",
      "publisherName": "Ваше Имя"
    },
    "linux": {
      "maintainer": "Ваше Имя evgenij.semenov.1998@bk.ru",
      "target": [
        "AppImage"
      ],
      "icon": "electron/ico/icon.png",
      "category": "Utility"
    },
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
